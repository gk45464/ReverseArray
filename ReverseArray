package com.Govinda;
public class Main {

    public static void main(String[] args) {
        int a[] = new int[]{6, 4, 4, 43, 9};
        System.out.println("Elements are : ");
        for (int i = 0; i < a.length; i++) {
            System.out.println(a[i]);
        }
        System.out.println("Reverse aRRay");
        for(int i= a.length - 1;i>=0;i--)
        {
            System.out.println(a[i]);
        }
    }



}
